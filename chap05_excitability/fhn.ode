v(0)=0
w(0)=0

par Iapp=0
par alpha=0.1, eps=0.01, gamma=0.5

v'= (v*(1-v)*(v-alpha) - w + Iapp)/eps
w'=v-gamma*w 

@ xpar=v, ypar=w,xlo=-0.3,xhi=1, ylo=-0.1, yhi=0.25
@ autovar=Iapp,ntst=50,nmax=2000,npr=2000,dsmin=0.01,dsmax=0.05,ds=0.02
@ parmin=0,parmax=1.5
@ autoxmin=0,autoxmax=1.5,autoymin=-0.4,autoymax=1.1,nmax=20000

done